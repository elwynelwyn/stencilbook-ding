{
    "name": "stencilbook-ding",
    "version": "0.0.1",
    "description": "Storybook + Stencil starter repo",
    "license": "MIT",
    "author": "elwyn",
    "repository": "https://github.com/elwynelwyn/stencilbook-ding",
    "files": [
        "dist-stencil"
    ],
    "scripts": {
        "start": "start-storybook -p 6009 --ci --static-dir ./dist-stencil",
        "build": "stencil build --ci",
        "prestencil:start-via-storybook": "shx rm -rf ./dist-stencil",
        "stencil:start-via-storybook": "stencil build --docs --docs-vscode ./dist-stencil/stencilbook-ding.json --watch --serve --no-open --dev",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "start-storybook": "npm run start",
        "prebuild-storybook": "npm run build",
        "build-storybook": "build-storybook --output-dir ./dist-storybook --static-dir ./dist-stencil"
    },
    "main": "dist-stencil/index.cjs.js",
    "module": "dist-stencil/index.js",
    "es2015": "dist-stencil/esm/index.mjs",
    "es2017": "dist-stencil/esm/index.mjs",
    "types": "dist-stencil/types/index.d.ts",
    "collection": "dist-stencil/collection/collection-manifest.json",
    "collection:main": "dist-stencil/collection/index.js",
    "unpkg": "dist-stencil/stencilbook-ding/stencilbook-ding.js",
    "dependencies": {
        "@stencil/core": "^2.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@storybook/addon-a11y": "^6.0.21",
        "@storybook/addon-essentials": "^6.0.21",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/web-components": "^6.0.21",
        "@types/jest": "^26.0.12",
        "@types/puppeteer": "^3.0.1",
        "babel-loader": "^8.1.0",
        "execa": "^4.0.3",
        "jest": "^26.4.2",
        "jest-cli": "^26.4.2",
        "lit-html": "^1.3.0",
        "modify-source-webpack-plugin": "^1.0.1",
        "prepend-transform": "^0.0.1019",
        "puppeteer": "^5.2.1",
        "react-is": "^16.13.1",
        "shx": "^0.3.2",
        "typescript": "4.0.2"
    }
}
